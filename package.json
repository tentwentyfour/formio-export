{
  "name": "formio-export",
  "version": "0.3.0",
  "description": "Formio Export Tools",
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:fast": "rm -rf dist && tsc",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify --clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "mocha --require jsdom-global/register './test/*.spec.js'",
    "test:watch": "mocha -w --require jsdom-global/register './test/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airarrazaval/formio-export.git"
  },
  "keywords": [
    "javascript",
    "formio",
    "formiojs"
  ],
  "author": "Alfredo Irarrazaval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airarrazaval/formio-export/issues"
  },
  "homepage": "https://github.com/airarrazaval/formio-export#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@types/node": "^20.19.33",
    "chai": "^4.5.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.8.2",
    "tsup": "^8.5.1",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "html2pdf.js": "^0.14.0",
    "lodash": "^4.17.23"
  },
  "volta": {
    "node": "20.19.0"
  }
}
